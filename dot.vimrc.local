set runtimepath+=~/.vim.local
color dusk
let mapleader = ","
map <silent> <F2> :bp<cr>
map <silent> <F3> :bn<cr>
map <silent> <F4> :CommandT<CR>
map <silent> <F5> :CommandTBuffer<CR>
map <Esc><Esc> :nohlsearch<CR><Esc>
inoremap <C-j> <Down>
inoremap <C-k> <Up>
inoremap <C-h> <Left>
inoremap <C-l> <Right>
nnoremap j gj
nnoremap k gk

"行間隔
set lsp=1
