set runtimepath+=~/.vim.local
color dusk
let mapleader = ","
map <silent> <F2> :bp<cr>
map <silent> <F3> :bn<cr>
map <Esc><Esc> :nohlsearch<CR><Esc>
inoremap <C-j> <Down>
inoremap <C-k> <Up>
inoremap <C-h> <Left>
inoremap <C-l> <Right>
nnoremap j gj
nnoremap k gk
nnoremap <silent> <Space>t :CommandT<Return>
"nmap <silent> <C-2> :CommandTBuffer<CR>

"行間隔
set lsp=1
